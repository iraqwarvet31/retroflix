<div class="container">
  <div class="row vertical-center">
    <body>
      <video
        id="my-video"
        class="video-js"
        controls
        preload="auto"
        width="640"
        height="264"
        data-setup="{}"
      >
        <source src="https://localhost:3000/video" type="video/mp4" />
        <p class="vjs-no-js">
          To view this video please enable JavaScript, and consider upgrading to a
          web browser that
          <a href="/video" target="_blank"
            >supports HTML5 video</a
          >
        </p>
      </video>
  </div>
</div>

